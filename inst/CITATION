citHeader("To cite martini in publications use:")

citEntry(entry = "Misc",
  title        = "martini: GWAS incorporating networks in R",
  author       = personList(as.person("Hector Climente-Gonzalez"),
                   as.person("Chloe-Agathe Azencott")),
  year         = "2017",
  url          = "https://doi.org/doi:10.18129/B9.bioc.martini",

  textVersion  =
  paste("Hector Climente-Gonzalez, Chloe-Agathe Azencott (2017).",
        "martini: GWAS incorporating networks in R.",
        "URL https://doi.org/doi:10.18129/B9.bioc.martini")

)

